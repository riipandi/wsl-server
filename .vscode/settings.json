{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "workbench.colorTheme": "One Dark Pro",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.activityBar.visible": true,
    "workbench.editor.showIcons": true,
    "workbench.editor.tabCloseButton": "right",
    "workbench.startupEditor": "none",
    "workbench.statusBar.feedback.visible": false,
    "workbench.enableExperiments": false,
    "workbench.tree.horizontalScrolling": false,
    "workbench.tips.enabled": false,
    "workbench.settings.editor": "json",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "typescript.surveys.enabled": false,
    "editor.insertSpaces": true,
    "editor.detectIndentation": true,
    "editor.formatOnType": false,
    "editor.renderControlCharacters": true,
    "editor.multiCursorModifier": "alt",
    "editor.snippetSuggestions": "top",
    "editor.formatOnPaste": false,
    "editor.fontSize": 13,
    "editor.wordWrap": "off",
    "editor.autoIndent": false,
    "editor.find.autoFindInSelection": false,
    "editor.autoClosingBrackets": "always",
    "editor.links": false,
    "editor.fontFamily": "'SF Mono', 'Fira Code', 'Roboto Mono', 'monospace'",
    "editor.fontLigatures": true,
    "editor.formatOnSave": false,
    "editor.minimap.enabled": true,
    "editor.tabCompletion": "on",
    "window.menuBarVisibility": "default",
    "window.newWindowDimensions": "maximized",
    "window.titleBarStyle": "custom",
    "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1200,
    "files.trimTrailingWhitespace": true,
    "files.encoding": "utf8",
    "files.eol": "\n",
    "extensions.showRecommendationsOnlyOnDemand": false,
    "update.enableWindowsBackgroundUpdates": false,
    "update.showReleaseNotes": false,
    "extensions.autoCheckUpdates": true,
    "extensions.autoUpdate": true,
    "extensions.ignoreRecommendations": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "explorer.autoReveal": false,
    "explorer.openEditors.visible": 0,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.fontSize": 12,
    "terminal.explorerKind": "integrated",
    "terminal.external.windowsExec": "C:\\Windows\\System32\\bash.exe",
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\bash.exe",
    "material-icon-theme.showWelcomeMessage": false,
    "material-icon-theme.showUpdateMessage": false,
    "markdown.extension.toc.githubCompatibility": true,
    "git.autofetch": false,
    "html.autoClosingTags": true,
    "html.format.endWithNewline": true,
    "html.format.indentHandlebars": true,
    "auto-rename-tag.activationOnLanguage": ["html", "xml", "php", "javascript", "vue"],
    "auto-close-tag.activationOnLanguage": ["xml", "php", "blade", "ejs", "jinja", "javascript", "javascriptreact", "typescript", "typescriptreact", "vue", "HTML (Eex)"],
    "auto-close-tag.enableAutoCloseTag": true,
    "auto-close-tag.SublimeText3Mode": true,
    "auto-close-tag.enableAutoCloseSelfClosingTag": true,
    "[markdown]": {"editor.quickSuggestions": false},
    "todo-tree.defaultHighlight": {},
    "code-runner.executorMap": {
        "javascript": "node",
        "php": "C:\\Windows\\php.bat",
        "python": "python",
        "go": "go run",
        "html": "chrome.exe",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    },
    "php.executablePath": "C:\\Windows\\php.bat",
    "php.suggest.basic": false,
    "php.validate.enable": false,
    "php-cs-fixer.onsave": true,
    "php-cs-fixer.rules": "@PSR2",
    "php-cs-fixer.config": ".php_cs;.php_cs.dist",
    "php-cs-fixer.allowRisky": false,
    "php-cs-fixer.pathMode": "override",
    "php-cs-fixer.exclude": [],
    "php-cs-fixer.autoFixByBracket": true,
    "php-cs-fixer.autoFixBySemicolon": false,
    "php-cs-fixer.formatHtml": false,
    "php-cs-fixer.documentFormattingProvider": true,
    "[php]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "html": "html",
        "vue": "html",
        "ejs": "html",
        "blade" : "html"
    },
    "eslint.enable": false,
    "eslint.packageManager": "yarn",
    "eslint.validate": [ "javascript", "javascriptreact", "vue", { "language": "html", "autoFix": true } ],
    "emmet.triggerExpansionOnTab": false, // causing shift+tab error if true
    "path-intellisense.showHiddenFiles": false,
    "npm.validate.enable": true,
    "npm.runInTerminal": true,
	"npm-intellisense.importES6": true,
	"npm-intellisense.importQuotes": "'",
	"npm-intellisense.importLinebreak": ";\r\n",
	"npm-intellisense.importDeclarationType": "const",
    "javascript.validate.enable": false,
    "scssFormatter.useTabs": false,
    "scssFormatter.tabWidth": 4,
    "scssFormatter.singleQuote": true,
    "html-css-class-completion.enableEmmetSupport": false,
    "sass.disableAutoIndent": false,
    "importCost.debug": false,
    "blade.format.enable": true
}
